    .navigators .navigators-ul {
        display: none;
        flex-direction: column;
        background-color: white;
        padding: 0.5rem calc(2vw + 5px);
    }
    
    .navigators .navigators-ul span {
        padding: 0.5rem 0;
    }
    
    .navigators .navigators-ul span a {
        color: black;
        text-decoration: none;
    }
    
    .make-ul-active-btn {
        display: inline-block;
