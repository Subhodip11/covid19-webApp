    }
    
    .header h1 {
        font-size: calc(24px - 5%);
        display: flex;
        align-items: center;
    }
    
    .header h1 img {
        height: 24px;
        width: 24px;
    }
    
    .navigators {
        /*border: 1px solid black;*/
        width: fit-content;
        height: auto;
